{
  "name": "udata-front",
  "version": "1.0.0",
  "description": "Open Data Portal - French customization",
  "author": "Etalab",
  "scripts": {
    "dev": "vite build --watch",
    "start": "npm run dev",
    "build": "vite build",
    "test": "cypress run",
    "cypress": "cypress open",
    "lighthouse": "lhci autorun",
    "i18n:report": "vue-i18n-extract report",
    "i18n:extract": "npm run i18n:report -- --add",
    "serve": "udata serve",
    "prepare-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata generate-fixtures",
    "serve-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata serve",
    "prepare-and-serve-e2e": "npm run prepare-e2e && npm run serve-e2e",
    "ci": "start-server-and-test prepare-and-serve-e2e http://localhost:7000 test"
  },
  "dependencies": {
    "@sentry/tracing": "^6.10.0",
    "@sentry/vue": "^6.10.0",
    "@vueform/multiselect": "^2.3.1",
    "axios": "^0.26.1",
    "clipboard": "^2.0.8",
    "dayjs": "^1.9.6",
    "es-module-shims": "^1.6.3",
    "less": "^3.13.1",
    "mitt": "^3.0.0",
    "popmotion": "^8.7",
    "prettier": "^2.2.1",
    "remove-markdown": "^0.3.0",
    "vue": "^3.2.25",
    "vue-content-loader": "^2.0.0",
    "vue-i18n": "^9.0.0-rc.2",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@conciergerie.dev/select-a11y": "^3.1.0",
    "@conciergerie.dev/vue-toaster": "^2.4.0",
    "@etalab/udata-front-plugins-helper": "1.1.0",
    "@gouvfr/dsfr": "^1.5.1",
    "@intlify/unplugin-vue-i18n": "^0.8.0",
    "@lhci/cli": "^0.10.0",
    "@vitejs/plugin-legacy": "^2.3.1",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.11",
    "axe-core": "^4.3.3",
    "browserslist": "^4.0.0",
    "consolidate": "^0.16.0",
    "cypress": "^12.5.1",
    "cypress-axe": "^1.3.0",
    "markdown-it": "^12.3.2",
    "postcss": "^8.4.12",
    "postcss-normalize": "^10.0.1",
    "process": "^0.11.10",
    "rollup-plugin-copy": "^3.4.0",
    "start-server-and-test": "^1.15.4",
    "terser": "^5.15.1",
    "vite": "^3.2.3",
    "vue-i18n-extract": "^2.0.7"
  },
  "license": "MIT"
}
