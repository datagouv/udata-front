{
  "name": "udata-front",
  "version": "3.5.1.dev",
  "description": "Open Data Portal - French customization",
  "author": {
    "name": "Etalab"
  },
  "scripts": {
    "dev": "vite build --watch --config vite.config.dev.ts",
    "start": "npm run dev",
    "build": "vite build",
    "test": "cypress run",
    "cypress": "cypress open",
    "lighthouse": "lhci autorun",
    "i18n:report": "vue-i18n-extract report",
    "i18n:extract": "npm run i18n:report -- --add",
    "serve": "udata serve",
    "prepare-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata generate-fixtures",
    "serve-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata serve",
    "prepare-and-serve-e2e": "npm run prepare-e2e && npm run serve-e2e",
    "ci": "start-server-and-test prepare-and-serve-e2e http://localhost:7000 test",
    "local-testing": "start-server-and-test prepare-and-serve-e2e http://localhost:7000 cypress",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic"
  },
  "overrides": {
    "fsevents": "^2.3.3"
  },
