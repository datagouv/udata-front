{
  "name": "udata-front",
  "version": "1.0.0",
  "description": "Open Data Portal - French customization",
  "author": "Etalab",
  "scripts": {
    "dev": "npm run clean && npm run watch",
    "build": "npm run clean && npm run build:stylemark && npm run build:app",
    "build:app": "parcel build theme/less/index.less theme/js/index.js --dist-dir udata_front/theme/gouvfr/static/",
    "build:stylemark": "npm run build:app -- --no-optimize",
    "test": "cypress run",
    "i18n:report": "vue-i18n-extract report --vueFiles 'theme/**/*.vue' --languageFiles 'theme/js/locales/**/*' --separator '^'",
    "i18n:extract": "npm run i18n:report -- --add",
    "clean": "rimraf .parcel-cache",
    "start": "npm run dev",
    "watch": "parcel theme/js/index.js theme/less/index.less --dist-dir udata_front/theme/gouvfr/static/ --host=dev.local --port=1234 --hmr-port=1235"
  },
  "dependencies": {
    "@meforma/vue-toaster": "^1.2.0",
    "@parcel/transformer-less": "^2.0.0",
    "@sentry/tracing": "^6.10.0",
    "@sentry/vue": "^6.10.0",
    "@vueform/multiselect": "^2.3.1",
    "axios": "^0.21.0",
    "clipboard": "^2.0.8",
    "dayjs": "^1.9.6",
    "less": "^3.13.1",
    "mitt": "^3.0.0",
    "parcel-reporter-copystatic": "^1.1.0",
    "parcel-reporter-stylemark": "^1.1.0",
    "popmotion": "^8.7",
    "prettier": "^2.2.1",
    "query-string": "^6.14.1",
    "remove-markdown": "^0.3.0",
    "vue": "^3.2.11",
    "vue-content-loader": "^2.0.0",
    "vue-final-modal": "^3.4.1",
    "vue-i18n": "^9.0.0-rc.2",
    "vue3-datepicker": "^0.2.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@conciergerie-dev/select-a11y": "^1.5.0",
    "@gouvfr/dsfr": "^1.2.0",
    "@parcel/transformer-inline-string": "^2.3.2",
    "@parcel/transformer-sass": "^2.3.2",
    "@parcel/transformer-stylus": "^2.3.2",
    "@parcel/transformer-vue": "^2.3.2",
    "@vue/compiler-sfc": "^3.2.11",
    "autoprefixer": "^9.8.6",
    "axe-core": "^4.3.3",
    "browserslist": "^4.0.0",
    "consolidate": "^0.16.0",
    "cssnano": "^4.1.10",
    "cypress": "^8.4.0",
    "cypress-axe": "^0.13.0",
    "markdown-it": "^12.2.0",
    "parcel": "^2.0.0",
    "postcss": "^8.2.3",
    "postcss-normalize": "^9.0.0",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "stylemark": "^3.1.7",
    "vue-i18n-extract": "^2.0.4"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "theme/img",
        "staticOutDir": "udata_front/theme/gouvfr/static/img/"
      }
    ]
  },
  "stylemarkPath": {
    "input": "theme/",
    "output": "udata_front/theme/gouvfr/static/stylemark/"
  },
  "alias": {
    "svg": "./udata_front/theme/gouvfr/templates/svg",
    "vue": "vue/dist/vue.cjs.js"
  },
  "license": "MIT"
}
