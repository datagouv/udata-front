{
  "name": "udata-front",
  "version": "4.0.4",
  "description": "Open Data Portal - French customization",
  "author": {
    "name": "Etalab"
  },
  "scripts": {
    "dev": "vite build --watch --config vite.config.dev.ts",
    "start": "npm run dev",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "test": "CYPRESS_INTERNAL_NO_TYPESCRIPT=1 cypress run",
    "cypress": "CYPRESS_INTERNAL_NO_TYPESCRIPT=1 cypress open",
    "lighthouse": "lhci autorun",
    "i18n:report": "vue-i18n-extract report",
    "i18n:extract": "npm run i18n:report -- --add",
    "serve": "udata serve",
    "prepare-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata import-fixtures",
    "serve-e2e": "UDATA_SETTINGS=${INIT_CWD}/cypress/udata-front-e2e.cfg udata serve",
    "prepare-and-serve-e2e": "npm run prepare-e2e && npm run serve-e2e",
    "local-testing": "start-server-and-test prepare-and-serve-e2e http://localhost:7000 cypress",
    "ci": "start-server-and-test prepare-and-serve-e2e http://localhost:7000 test",
    "ci:storybook": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --stats-json",
    "serve-storybook": "npx http-server storybook-static --port 6006 --silent",
    "test-storybook": "test-storybook",
    "chromatic": "npx chromatic --only-changed --storybook-build-dir ./storybook-static --externals '**/*.less' --externals '**/*.json' --externals '**/msw-requests.ts'"
  },
  "overrides": {
    "fsevents": "^2.3.3"
  },
  "devDependencies": {
    "@conciergerie.dev/vue-toaster": "^2.4.4",
    "@etalab/udata-front-plugins-helper": "1.1.0",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@lhci/cli": "^0.14.0",
    "@milkdown/components": "^7.3.3",
    "@milkdown/core": "^7.3.3",
    "@milkdown/ctx": "^7.3.3",
    "@milkdown/plugin-history": "^7.3.3",
    "@milkdown/plugin-listener": "^7.3.3",
    "@milkdown/plugin-tooltip": "^7.3.3",
    "@milkdown/preset-commonmark": "^7.3.3",
    "@milkdown/preset-gfm": "^7.3.3",
    "@milkdown/prose": "^7.3.3",
    "@milkdown/transformer": "^7.3.3",
    "@milkdown/vue": "^7.3.3",
    "@prosemirror-adapter/vue": "^0.2.6",
    "@sentry/tracing": "^6.10.0",
    "@sentry/vue": "^6.10.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/addon-themes": "^8.1.6",
    "@storybook/blocks": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.3.0",
    "@storybook/vue3": "^8.3.0",
    "@storybook/vue3-vite": "^8.3.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/core": "^10.5.0",
    "axe-core": "^4.3.3",
    "browserslist": "^4.21.5",
    "chart.js": "^4.2.1",
    "chromatic": "^11.5.1",
    "clipboard": "^2.0.11",
    "cypress": "^13.1.0",
    "cypress-axe": "^1.5.0",
    "date-fns": "^2.30.0",
    "es-module-shims": "^1.7.0",
    "glob": "^10.2.2",
    "iconoir": "^7.4.0",
    "jest-playwright-preset": "^4.0.0",
    "less": "^4.1.3",
    "markdown-it": "^12.3.2",
    "mitt": "^3.0.0",
    "msw": "^2.4.8",
    "msw-storybook-addon": "^2.0.3",
    "playwright": "^1.46.1",
    "popmotion": "^8.7",
    "postcss": "^8.4.12",
    "postcss-normalize": "^10.0.1",
    "remove-markdown": "^0.5.0",
    "rollup-plugin-copy": "^3.5.0",
    "start-server-and-test": "^2.0.5",
    "storybook": "^8.3.0",
    "terser": "^5.15.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vite": "^4.5.2",
    "vue": "^3.5.6",
    "vue-content-loader": "^2.0.1",
    "vue-i18n": "^9.13.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-router": "^4.2.5",
    "vue3-datepicker": "^0.4.0"
  },
  "license": "LGPL-2.1-only",
  "dependencies": {
    "@datagouv/components": "file:udata_front/theme/gouvfr/datagouv-components",
    "@datagouv/select-a11y": "^3.6.1",
    "@gouvfr/dsfr": "~1.11.2",
    "@vueuse/integrations": "^10.11.0",
    "axios": "^1.7.7",
    "leaflet": "^1.9.4",
    "oh-vue-icons": "^1.0.0-rc3",
    "sortablejs": "^1.15.2",
    "vue3-text-clamp": "^0.1.2"
  },
  "msw": {
    "workerDirectory": [
      ".storybook/msw"
    ]
  }
}
